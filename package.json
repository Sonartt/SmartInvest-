    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
