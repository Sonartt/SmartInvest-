{
  "name": "smartinvest",
  "version": "1.0.0",
  "description": "SmartInvest - Investment Management Platform",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run prisma:generate",
    "start": "node dist/server.js",
    "start:server.js": "node server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prisma:generate": "prisma generate",
